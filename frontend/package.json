{
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "cd .. && husky install frontend/.husky",
    "ncu:all": "turbo run ncu",
    "ncu": "ncu -u --timeout 120000",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "build": "turbo run build",
    "pretty-quick": "pretty-quick --staged",
    "prettier": "prettier --write --ignore-unknown ./**/*.{ts,css,less,scss}",
    "clean": "turbo run clean && rimraf /s/d node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.0",
    "@fa/eslint-config-custom": "workspace:*",
    "@types/node": "18.15.3",
    "eslint": "8.36.0",
    "husky": "8.0.3",
    "npm-check-updates": "16.7.12",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "pretty-quick": "3.1.3",
    "rimraf": "4.4.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.18.2"
}
